{
  "name": "esthers",
  "version": "1.0.0",
  "description": "Webshop for company Esthers",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "NODE_PATH=./lib/client pm2 start lib/server --watch --interpreter babel-node",
    "pm2-delete": "pm2 delete all",
    "webpack": "webpack -w",
    "build-node": "babel lib -d build --copy-files",
    "build-webpack": "webpack",
    "start": "NODE_ENV=production NODE_PATH=./build/client pm2-runtime start ecosystem.config.js --env production",
    "build": "concurrently \"npm run build-node\" \"npm run build-webpack\""
  },
  "heroku-run-build-script": true,
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ]
  },
  "author": "Christian Frahm Sahlstr√∂m",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.4.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "pm2": "^5.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.5"
  }
}
